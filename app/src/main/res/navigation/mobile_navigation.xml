<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home"
    tools:ignore="UnusedNavigation">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.sirem.jadwalsolat.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_solat"
        android:name="com.sirem.jadwalsolat.ui.solat.SolatFragment"
        android:label="Solat"
        tools:layout="@layout/fragment_solat" />

    <fragment
        android:id="@+id/navigation_quran"
        android:name="com.sirem.jadwalsolat.ui.quran.QuranFragment"
        android:label="Quran"
        tools:layout="@layout/fragment_quran" />
    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.sirem.jadwalsolat.ui.profile.ProfileFragment"
        android:label="Profile"
        tools:layout="@layout/fragment_profile" />
</navigation>